@startuml
start

:初始化公鸡数量;
:初始化母鸡数量;
:初始化小鸡数量;
:初始化解决方案数量;

while (公鸡数量 * 5 <= 100) is (条件1)
  while (母鸡数量 * 3 <= 100) is (条件2)
    while (小鸡数量 / 3 <= 100) is (条件3)
      if (公鸡数量 + 母鸡数量 + 小鸡数量 == 100) then (条件4)
        :增加解决方案数量;
      endif
      :增加小鸡数量;
    endwhile
    :增加母鸡数量;
  endwhile
  :增加公鸡数量;
endwhile

:输出解决方案数量;

stop
@enduml
